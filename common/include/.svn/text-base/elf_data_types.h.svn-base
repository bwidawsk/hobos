#ifndef _ELF_DATA_TYPES_H_
#define _ELF_DATA_TYPES_H_

/* Size Alignment  Desc*/
typedef uint32_t Elf32_Addr;   /* 4	4	Unsigned program address */
typedef uint16_t Elf32_Half;	/* 2	2 	Unsigned medium integer  */
typedef uint32_t Elf32_Off; 	/*4 4 Unsigned file offset*/
typedef int32_t  Elf32_Sword; 	/*4 4 Signed large integer*/
typedef uint32_t Elf32_Word;	/*  4 4 Unsigned large integer */

typedef uint64_t Elf64_Addr; /* 8 8 */
typedef uint16_t Elf64_Half; /* 2 2 */
typedef int16_t  Elf64_SHalf;/* 2 2 */
typedef uint64_t Elf64_Off;/* 8 8 */
typedef int32_t  Elf64_Sword;/* 4 4 */
typedef uint32_t Elf64_Word;/* 4 4 */
typedef uint64_t Elf64_Xword;/* 8 8 */
typedef int64_t  Elf64_Sxword;/* 8 8 */

#endif // _ELF_DATA_TYPES_H_
